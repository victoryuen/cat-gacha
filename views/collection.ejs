<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Comfortaa:wght@300;400;500;600;700&family=Montserrat:wght@400&display=swap"
        rel="stylesheet">
    <title>Gacha</title>
</head>

<body>
    <%- include('partials/nav') %>
        <% if (locals.loggedIn) { %>
            <div class="middle-container">
                <h1>Collection</h1>
                <div id="collection-section" class="cat-grid">
                    <% cats.forEach(cat=> { %>
                        <div class="cat-card">
                            <h2>
                                <%= cat.cat_name %>
                            </h2>
                            <img class="cat-collection-img" src="<%= cat.cat_image %>" alt="<%= cat.cat_name %>">
                        </div>
                        <% }) %>
                </div>
            </div>
            <% } %>
</body>

</html>